{
  "name": "ascstudymonitor-client",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "NODE_ENV=production vue-cli-service build",
    "build:dev": "NODE_ENV=development vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:watch": "vue-cli-service test:unit --watch",
    "coveralls": "coveralls < ./coverage/lcov.info",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@sentry/browser": "^5.15.5",
    "@sentry/integrations": "^5.15.5",
    "core-js": "^3.6.5",
    "debug": "^4.1.1",
    "fuse.js": "^6.0.0",
    "localforage": "^1.7.3",
    "qs": "^6.9.4",
    "raf": "^3.4.1",
    "v-hotkey": "^0.8.0",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.3.1",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-router": "^3.2.0",
    "vue-slide-up-down": "^2.0.1",
    "vue-social-sharing": "^3.0.0-beta.11",
    "vue-tailwind": "^1.0.2",
    "vue-toasted": "^1.1.28",
    "vue-typer": "^1.2.0",
    "vuejs-paginate": "^2.1.0",
    "vuex": "^3.4.0",
    "vuex-persist": "^2.2.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.4.1",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "git-rev-sync": "^3.0.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "postcss-assets": "^5.0.0",
    "postcss-nested": "^4.2.3",
    "tailwindcss": "^1.6.2",
    "vue-template-compiler": "^2.6.11",
    "wallaby-vue-compiler": "^1.0.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "setupFiles": [
      "<rootDir>/jest.init.js"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/src/**/*.{js,vue}",
      "!**/node_modules/**",
      "!**/dist/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/membranepotential/ascstudymonitor.git"
  }
}
